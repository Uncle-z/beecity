          <view class="td">2014-02-01 15:01:01</view>
          <view class="td"><van-button type="info">详情</van-button></view>
        </view>
      </view>
    </scroll-view>
  </view>
  <view class="ui-row" style="height: 80px;">
    <view class="ui-col ui-col-align-center align-center valign-middle">
      <view class="ui-col-content align-center valign-middle">
        <van-button type="info" size="small">审核通过</van-button>
      </view>
    </view>
    <view class="ui-col ui-col-align-center align-center valign-middle">
      <view class="ui-col-content align-center valign-middle">
        <van-button type="warning" size="small">审核不通过</van-button>
      </view>
    </view>
  </view>
